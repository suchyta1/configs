# Get rid of stupid noises when attempting to tab complete
#set bell-style none

# Enable tab completion for git
source ~/.git-completion.bash

# Use Liquid Prompt (only in interactive shells, not from a script or from scp)
#[[ $- = *i* ]] && source ~/liquidprompt/liquidprompt

# Needed to colorize gnu-screen
force_color_prompt=yes

# Aliases so colors show up on commands I use
alias ls="ls --color=auto"
alias grep="grep --color=auto"
